package test;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.firefox.FirefoxDriver;

public class FirstSeleniumTest {

	WebDriver driver;
	
	// Use case: Launch a Firefox Browser and visit the Amazon site.
	public void launchBroser() { 
		System.setProperty("webdriver.chrome.driver", "C:\\Users\\USER\\OneDrive\\Desktop\\Stefan\\Fax\\III godina\\6. Leten semestar\\SKT\\proj\\Installers\\driver\\chromedriver.exe");
		driver = new ChromeDriver();
		driver.get("https://www.ebay.com/");
	}
   
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		FirstSeleniumTest obj = new FirstSeleniumTest();
		obj.launchBroser();
	}

}
